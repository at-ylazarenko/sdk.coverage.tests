{
  "name": "@applitools/coverage-tests",
  "private": true,
  "version": "1.0.0",
  "engines": {
    "node": ">=10.0.0"
  },
  "main": "index.js",
  "description": "Coverage Tests for Applitools SDKs",
  "author": "Applitools <team@applitools.com>",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/applitools/sdk.coverage.tests.git"
  },
  "scripts": {
    "java:create": "cd java && coverage-tests create-tests --path ./configuration",
    "ruby:test": "yarn ruby:create && yarn ruby:run:parallel && yarn ruby:report:merge && yarn ruby:report",
    "ruby:create": "cd ruby && coverage-tests create-tests --path ./configuration",
    "ruby:report": "cd ruby && coverage-tests process-report --path ./configuration",
    "ruby:run": "cd ruby && APPLITOOLS_BATCH_NAME='Ruby Coverage Tests' APPLITOOLS_BATCH_ID=$(uuidgen) bundle exec rspec spec/coverage/generic",
    "ruby:run:parallel": "cd ruby && APPLITOOLS_BATCH_NAME='Ruby Coverage Tests' APPLITOOLS_BATCH_ID=$(uuidgen) bundle exec parallel_rspec -n 8 spec/coverage/generic",
    "ruby:report:merge": "cd ruby && junit-merge -d tmp -o coverage-test-report.xml",
    "ruby:conf:test": "mocha './ruby/configuration/test/*.spec.js'",
    "python": "cd python && coverage-tests create-tests --path ."
  },
  "dependencies": {
    "@applitools/sdk-coverage-tests": "1.0.5",
    "junit-merge": "^2.0.0",
    "@typescript-eslint/parser": "^2.14.0",
    "typescript": "^3.7.4"
  },
  "devDependencies": {
    "mocha": "^8.0.1"
  }
}
